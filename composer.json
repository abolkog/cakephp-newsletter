{
    "name": "abolkog/Newsletter",
    "description": "Newsletter plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepae": "https://github.com/abolkog/cakephp-newsletter",
    "keywords": ["cakephp","Newsletter","Mailing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Khalid Elshafie",
            "homepage": "http://www.abolkog.com",
            "role": "Contributor"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Newsletter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Newsletter\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
